{
    "name": "docker-login",
    "description": "GitHub Action to login against a Docker registry",
    "main": "src/main.ts",
    "type": "module",
    "engines": {
      "node": ">=18.19.0"
    },
    "scripts": {
      "build": "npx ncc build --source-map --minify --license licenses.txt",
      "lint": "npm run prettier && npm run eslint",
      "format": "npm run prettier:fix && npm run eslint:fix",
      "eslint": "npx eslint --max-warnings=0 .",
      "eslint:fix": "npx eslint --fix .",
      "prettier": "npx prettier --check \"./**/*.ts\"",
      "prettier:fix": "npx prettier --write \"./**/*.ts\"",
      "test": "npx jest --config=jest.config.ts",
      "test:integration": "jest --config=jest.config.ts \"__tests__/integration/**/*.test.ts\" --runInBand --detectOpenHandles --forceExit",
      "test:coverage": "npx jest --config=jest.config.ts --coverage",
      "security-test": "npx jest --config=jest.config.ts \"__tests__\\integration\\auth-security.tests.ts\"",
      "security-test:watch": "npx jest --config=jest.config.ts \"__tests__\\integration\\auth-security.tests.ts\" --watch",
      "test:ci": "npx jest --config=jest.config.ts --ci --coverage --reporters=\"default\" --reporters=\"jest-junit\"",
      "test:system": "npx jest --config=jest.config.ts --testMatch \"**\\__tests__\\integration\\system.test.ts\"",
      "test:all": "npm run test && npm run test:system",
      "metrics": "npm run test:coverage && node scripts\\generate-metrics.js",
      "security:audit": "npm audit",
      "security:scan": "npx zap-cli quick-scan --self-contained",
      "complexity": "npx eslint \"src\\**\\*.ts\" --config .eslintrc.complexity.json",
      "validate": "npm run lint && npm run test:coverage && npm run security:audit",
      "monitor": "npx snyk monitor",
      "analyze": "npx eslint . --format html -o reports\\lint-report.html",
      "test-and-report": "npx jest --config=jest.config.ts --coverage --reporters=default --reporters=jest-junit && node scripts\\generate-report.mjs",
      "generate-report": "node scripts\\generate-report.mjs",
      "pretest": "npm run lint",
      "test:full": "npm run test:all && npm run security-test && npm run metrics",
      "report:full": "npm run test:full && npm run report",
      "ci": "npm run test:full && npm run report && npm run complexity",
      "prepare": "npx husky install"
    },
    "dependencies": {
      "@actions/core": "^1.11.1",
      "@actions/exec": "^1.1.1",
      "@actions/github": "^5.1.1",
      "@actions/io": "^1.1.3",
      "@aws-sdk/client-ecr": "^3.723.0",
      "@aws-sdk/client-ecr-public": "^3.723.0",
      "@docker/actions-toolkit": "^0.42.0",
      "@smithy/middleware-retry": "^2.1.1",
      "http-proxy-agent": "^7.0.2",
      "https-proxy-agent": "^7.0.5"
    },
    "devDependencies": {
      "@jest/globals": "^29.7.0",
      "@types/jest": "^29.5.14",
      "@types/node": "^20.17.9",
      "@types/yargs": "^17.0.33",
      "@types/yargs-parser": "^21.0.3",
      "@typescript-eslint/eslint-plugin": "^7.9.0",
      "@typescript-eslint/parser": "^7.9.0",
      "@vercel/ncc": "^0.38.1",
      "@xmldom/xmldom": "^0.8.10",
      "dotenv": "^16.4.5",
      "eslint": "^8.57.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-complexity": "^1.0.0",
      "eslint-plugin-jest": "^28.5.0",
      "eslint-plugin-prettier": "^5.1.3",
      "eslint-plugin-security": "^1.7.1",
      "eslint-plugin-sonarjs": "^0.23.0",
      "husky": "^8.0.3",
      "jest": "^29.7.0",
      "jest-environment-node": "^29.7.0",
      "jest-junit": "^16.0.0",
      "jsdom": "^23.0.1",
      "prettier": "^3.2.5",
      "snyk": "^1.1130.0",
      "testcontainers": "^10.15.0",
      "ts-jest": "^29.2.5",
      "ts-node": "^10.9.2",
      "typescript": "^5.3.3"
    }
  }
  