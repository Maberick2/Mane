{
  "name": "docker-login",
  "description": "GitHub Action to login against a Docker registry",
  "main": "src/main.ts",
  "scripts": {
    "build": "ncc build --source-map --minify --license licenses.txt",
    "lint": "npm run prettier && npm run eslint",
    "format": "npm run prettier:fix && npm run eslint:fix",
    "eslint": "eslint --max-warnings=0 .",
    "eslint:fix": "eslint --fix .",
    "prettier": "prettier --check \"./**/*.ts\"",
    "prettier:fix": "prettier --write \"./**/*.ts\"",
    "test": "jest",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "security-test": "jest \"__tests__/integration/auth-security.tests.ts\"",
    "security-test:watch": "jest \"__tests__/integration/auth-security.tests.ts\" --watch",
    "test:ci": "jest --ci --coverage --reporters='default' --reporters='jest-junit'",
    "test:system": "jest --config=jest.config.ts --testMatch \"**/__tests__/integration/system.test.ts\"",
    "test:all": "npm run test && npm run test:system",
    "metrics": "npm run test:coverage && node scripts/generate-metrics.js",
    "security:audit": "npm audit && snyk test",
    "security:scan": "zap-cli quick-scan --self-contained",
    "complexity": "eslint . --ext .ts --rule \"complexity: [2, 10 ]\"",
    "validate": "npm run lint && npm run test:coverage && npm run security:audit",
    "monitor": "snyk monitor",
    "analyze": "npx eslint-detailed-reporter",
    "prepare": "husky && husky install"
  },
  "dependencies": {
    "@aws-sdk/client-ecr": "^3.664.0",
    "@aws-sdk/client-ecr-public": "^3.664.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.5"
  },
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@docker/actions-toolkit": "^0.46.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.9",
    "@types/yargs": "^17.0.33",
    "@types/yargs-parser": "^21.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vercel/ncc": "^0.38.1",
    "@xmldom/xmldom": "^0.9.5",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-detailed-reporter": "^0.9.0",
    "eslint-plugin-complexity": "^1.0.2",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.1",
    "snyk": "^1.1130.0",
    "testcontainers": "^10.15.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
