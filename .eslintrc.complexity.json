{  
  "env": {  
    "node": true,  
    "es6": true  
  },  
  "parser": "@typescript-eslint/parser",  
  "parserOptions": {  
    "ecmaVersion": 2020,  
    "sourceType": "module",  
    "project": "./tsconfig.json"  
  },  
  "plugins": [  
    "@typescript-eslint",  
    "complexity"  
  ],  
  "rules": {  
    "complexity": ["error", 10]  
  }  
}